    def findTwoNumbers(self, nums):
        carry = 0
        root = n = ListNode(0)
        while nums or carry:
            v1 = v2 = 0
            if nums:
                v1 = nums[0]
                nums = nums[1:]
            if carry:
                v2 = nums[-1]
